# Example environment variables for WarpGen
# Webhook support has been removed in favor of Supabase stats tracking.

# Supabase integration to store generation counts (optional)
# Create a table named 'stats' with an 'id' (integer, primary key) 
# and 'total_generations' (integer, default 0)
# Insert a single row with id = 1 and total_generations = 0

SUPABASE_URL=
SUPABASE_KEY=

# Local fallback stats file
STATS_FILE=warpgen_stats.json
